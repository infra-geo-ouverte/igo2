    "scroll-into-view-if-needed": "^2.2.20",
    "stream": "^0.0.2",
    "tslib": "^1.9.3",
    "typy": "^2.0.1",
    "unorm": "^1.4.1",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.2",
