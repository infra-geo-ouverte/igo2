    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "live-server": "^1.2.1",
    "ng-packagr": "^3.0.0-rc.2",
    "npx": "^10.2.0",
    "protractor": "~5.4.2",
    "publish-release": "^1.6.0",
    "scss-bundle": "^2.5.0",
    "ts-node": "~8.0.2",
    "tsickle": "^0.34.2",
    "tslint": "^5.12.1",
