{
  "name": "igo",
  "version": "0.1.0",
  "description": "Infrastructure g√©omatique ouverte",
  "repository": {
    "url": "https://github.com/infra-geo-ouverte/igo2"
  },
  "license": "LiLiQ-R",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --proxy-config proxy.conf.json",
    "start.dev": "npm start",
    "start.prod": "ng serve --prod --host 0.0.0.0 --proxy-config proxy.conf.json",
    "build.dev": "ng build --dev --output-path ./dist/dev",
    "build.prod": "ng build --prod --output-path ./dist/prod",
    "build.prod.aot": "ng build --prod --output-path ./dist/prod",
    "serve.dev": "npm run build.dev && node_modules/live-server/live-server.js ./dist/dev/ --port=4200 --no-browser",
    "serve.prod": "npm run build.prod && node_modules/live-server/live-server.js ./dist/prod/ --port=4200 --no-browser",
    "serve.prod.aot": "npm run build.prod.aot && node_modules/live-server/live-server.js ./dist/prod/ --port=4200 --no-browser",
    "doc": "compodoc -p src/tsconfig.json",
    "serve.doc": "compodoc -p src/tsconfig.json -s --port 4220",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test --lint --single-run",
    "test.watch": "ng test --lint",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "coverage": "ng test --code-coverage --single-run",
    "serve.coverage": "npm run coverage && node_modules/live-server/live-server.js ./coverage/ --port=4210 --no-browser",
    "outdated": "ncu",
    "outdated.wizard": "npm-check -u",
    "audit-nsp": "nsp check package.json",
    "audit": "snyk test",
    "audit.wizard": "snyk wizard",
    "audit.protect": "snyk protect",
    "tests.all": "npm run audit && npm run test && npm run e2e",
    "cli-update": "npm cache clean && npm install --save-dev angular-cli@latest && npm run reinstall && ng init --routing --style=styl --prefix=igo",
    "reinstall": "npm cache clean && npm prune && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "core-js": "^2.4.1",
    "openlayers": "^3.20.1",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "@types/openlayers": "^3.20.4",
    "angular-cli": "^1.0.0-beta.26",
    "codelyzer": "~2.0.0-beta.1",
    "compodoc": "0.0.36",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "npm-check": "^5.4.0",
    "nsp": "^2.6.2",
    "protractor": "~4.0.13",
    "snyk": "^1.24.6",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3"
  },
  "snyk": true
}
