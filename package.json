{
  "name": "igo",
  "version": "0.6.0",
  "description": "Infrastructure g√©omatique ouverte",
  "repository": {
    "url": "https://github.com/infra-geo-ouverte/igo2"
  },
  "license": "LiLiQ-R",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --proxy-config proxy.conf.json",
    "start.dev": "npm start",
    "start.prod": "ng serve --aot --prod --host 0.0.0.0 --proxy-config proxy.conf.json",
    "build.dev.jit": "ng build --dev --output-path ./dist/dev",
    "build.dev": "ng build --output-path ./dist/dev --dev --bh /",
    "build.prod.jit": "ng build --prod --output-path ./dist/prod",
    "build.prod": "ng build --output-path ./dist/prod --prod --bh /",
    "serve.dev": "node_modules/live-server/live-server.js ./dist/dev/ --port=4200 --no-browser",
    "serve.prod": "node_modules/live-server/live-server.js ./dist/prod/ --port=4200 --no-browser",
    "doc": "compodoc -p src/tsconfig.app.json -s --port 4220",
    "build.doc": "compodoc -p src/tsconfig.app.json",
    "serve.doc": "compodoc -s --port 4220",
    "coverage": "npm run build.coverage && npm run serve.coverage",
    "build.coverage": "ng test --code-coverage --single-run",
    "serve.coverage": "node_modules/live-server/live-server.js ./coverage/ --port=4210 --no-browser",
    "i18n": "ng2-translate-extract --dir ./src --output ./src/assets/locale/ --format=json --clean",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.app.json && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.e2e.json",
    "pretest": "ng lint",
    "test": "ng test --single-run",
    "test.watch": "ng test",
    "pree2e": "webdriver-manager clean && webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "tests.all": "npm run test && npm run e2e",
    "outdated": "ncu",
    "outdated.wizard": "npm-check -u",
    "audit-nsp": "nsp check package.json",
    "audit": "snyk test",
    "audit.wizard": "snyk wizard",
    "audit.protect": "snyk protect",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "cli-update": "npm cache clean && npm install --save-dev angular-cli@latest && npm run reinstall && ng update --routing --style=styl --prefix=igo",
    "reinstall": "npm run clean && npm cache clean && npm prune && npm install",
    "clean": "rimraf ./dist",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "preversion": "npm run tests.all && npm run clean && npm run build.prod",
    "version": "npm run changelog && git add -A CHANGELOG.md",
    "postversion": "git push && git push --tags && rimraf igo2.zip && cd ./dist/prod/ && zip -r ../igo2.zip ./*"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "git+https://github.com/angular/material2-builds.git",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.1",
    "angular2-notifications": "^0.4.53",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "md2": "0.0.18",
    "ng2-translate": "^5.0.0",
    "openlayers": "^4.0.1",
    "proj4": "^2.4.3",
    "rxjs": "^5.1.0",
    "ts-md5": "^1.2.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@biesbjerg/ng2-translate-extract": "^0.6.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.60",
    "@types/openlayers": "^4.0.1",
    "codelyzer": "~2.0.0",
    "compodoc": "0.0.41",
    "conventional-changelog-cli": "^1.3.1",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-check": "^5.4.0",
    "nsp": "^2.6.3",
    "protractor": "~4.0.13",
    "rimraf": "^2.6.1",
    "snyk": "^1.26.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.1.0"
  },
  "snyk": true
}
