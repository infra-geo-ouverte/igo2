{
  "name": "igo",
  "version": "0.3.3",
  "description": "Infrastructure g√©omatique ouverte",
  "repository": {
    "url": "https://github.com/infra-geo-ouverte/igo2"
  },
  "license": "LiLiQ-R",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --proxy-config proxy.conf.json",
    "start.dev": "npm start",
    "start.prod": "ng serve --aot --prod --host 0.0.0.0 --proxy-config proxy.conf.json",
    "build.dev.jit": "ng build --dev --output-path ./dist/dev",
    "build.dev": "ng build --output-path ./dist/dev --dev --bh /",
    "build.prod.jit": "ng build --prod --output-path ./dist/prod",
    "build.prod": "ng build --output-path ./dist/prod --prod --bh /",
    "serve.dev": "node_modules/live-server/live-server.js ./dist/dev/ --port=4200 --no-browser",
    "serve.prod": "node_modules/live-server/live-server.js ./dist/prod/ --port=4200 --no-browser",
    "doc": "compodoc -p src/tsconfig.json -s --port 4220",
    "build.doc": "compodoc -p src/tsconfig.json",
    "serve.doc": "compodoc -s --port 4220",
    "coverage": "npm run build.coverage && npm run serve.coverage",
    "build.coverage": "ng test --code-coverage --single-run",
    "serve.coverage": "node_modules/live-server/live-server.js ./coverage/ --port=4210 --no-browser",
    "i18n": "ng2-translate-extract --dir ./src --output ./src/assets/locale/ --format=json --clean",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json",
    "pretest": "ng lint",
    "test": "ng test --single-run",
    "test.watch": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "tests.all": "npm run test && npm run e2e",
    "outdated": "ncu",
    "outdated.wizard": "npm-check -u",
    "audit-nsp": "nsp check package.json",
    "audit": "snyk test",
    "audit.wizard": "snyk wizard",
    "audit.protect": "snyk protect",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "cli-update": "npm cache clean && npm install --save-dev angular-cli@latest && npm run reinstall && ng update --routing --style=styl --prefix=igo",
    "reinstall": "npm run clean && npm cache clean && npm prune && npm install",
    "clean": "rimraf ./dist",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.4.5",
    "@angular/compiler": "2.4.5",
    "@angular/compiler-cli": "2.4.5",
    "@angular/core": "2.4.5",
    "@angular/forms": "2.4.5",
    "@angular/http": "2.4.5",
    "@angular/material": "2.0.0-beta.1",
    "@angular/platform-browser": "2.4.5",
    "@angular/platform-browser-dynamic": "2.4.5",
    "@angular/platform-server": "2.4.5",
    "@angular/router": "3.4.5",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.1",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "ng2-translate": "^5.0.0",
    "openlayers": "^3.20.1",
    "proj4": "^2.3.17",
    "rxjs": "^5.1.0",
    "snyk": "^1.25.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-beta.32.3",
    "@angular/compiler-cli": "^2.4.0",
    "@biesbjerg/ng2-translate-extract": "^0.6.0",
    "@types/jasmine": "^2.5.41",
    "@types/node": "^7.0.4",
    "@types/openlayers": "^3.20.4",
    "codelyzer": "~2.0.0-beta.4",
    "compodoc": "0.0.38",
    "conventional-changelog-cli": "^1.2.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "npm-check": "^5.4.0",
    "nsp": "^2.6.2",
    "protractor": "~4.0.13",
    "rimraf": "^2.5.4",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "typescript": "^2.1.5"
  },
  "snyk": true
}
