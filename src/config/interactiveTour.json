{
  "global": {
    "title": "interactiveTour.global.title",
    "position": "auto",
    "steps": [
      {
        "element": ".menu-button",
        "title": "interactiveTour.global.menu-button-title",
        "text": "interactiveTour.global.menu-button",
        "onHide": {
          "action": "click",
          "condition": "mat-sidenav:not(.mat-drawer-opened)"
        }
      },
      {
        "element": ".igo-search-bar-container",
        "title": "interactiveTour.global.search-bar-title",
        "text": "interactiveTour.global.search-bar"
      },
      {
        "element": ".search-bar-buttons",
        "title": "interactiveTour.global.search-settings-title",
        "text": "interactiveTour.global.search-settings",
        "disableInteraction": true
      },
      {
        "element": "igo-toolbox",
        "title": "interactiveTour.global.toolbox-title",
        "text": "interactiveTour.global.toolbox",
        "beforeShow": {
          "element": "#homeButton",
          "action": "click"
        }
      },
      {
        "element": "igo-actionbar-item:nth-child(2) mat-list-item",
        "title": "interactiveTour.global.context-title",
        "text": "interactiveTour.global.context",
        "disableInteraction": true,
        "beforeShow": {
          "element": "#homeButton",
          "action": "click"
        },
        "beforeChange": {
          "action": "click",
          "waitFor": ".igo-tool-container"
        }
      },
      {
        "element": ".igo-tool-container",
        "title": "interactiveTour.global.context-title",
        "text": "interactiveTour.global.context-list",
        "disableInteraction": true
      },
      {
        "element": "igo-context-item",
        "title": "interactiveTour.global.context-title",
        "text": "interactiveTour.global.context-item"
      },
      {
        "element": "igo-context-item:nth-of-type(3)",
        "title": "interactiveTour.global.context-title",
        "text": "interactiveTour.global.context-filter"
      },
      {
        "element": "igo-context-item:nth-of-type(3)",
        "title": "interactiveTour.global.context-title",
        "text": "interactiveTour.global.context-filter-click",
        "beforeChange": {
          "action": "click",
          "waitFor": "igo-list"
        }
      },
      {
        "element": "igo-list",
        "title": "interactiveTour.global.layers-title",
        "text": "interactiveTour.global.layers",
        "noBackButton": true,
        "beforeShow": {
          "action": "click",
          "element": "igo-actionbar-item:nth-child(3) mat-list-item"
        }
      },
      {
        "element": "igo-list",
        "title": "interactiveTour.global.layers-title",
        "text": "interactiveTour.global.layers-visible"
      },
      {
        "element": "igo-layer-item:nth-of-type(2)",
        "title": "interactiveTour.global.layers-title",
        "text": "interactiveTour.global.layer-embacle"
      },
      {
        "element": "igo-layer-item:nth-of-type(2) h4",
        "title": "interactiveTour.global.layers-title",
        "text": "interactiveTour.global.legend",
        "onShow": {
          "action": "click"
        }
      },
      {
        "element": "igo-layer-item:nth-child(2) button",
        "title": "interactiveTour.global.layers-title",
        "text": "interactiveTour.global.layer-eye",
        "onShow": {
          "action": "click"
        }
      },
      {
        "element": "igo-layer-item:nth-child(2) button:nth-of-type(2)",
        "title": "interactiveTour.global.layers-title",
        "text": "interactiveTour.global.layer-tool-btn",
        "beforeChange": {
          "action": "click",
          "waitFor": ".igo-layer-actions-container",
          "condition": "!.igo-layer-actions-container"
        }
      },
      {
        "element": ".igo-layer-actions-container",
        "title": "interactiveTour.global.layers-title",
        "text": "interactiveTour.global.layer-tool"
      },
      {
        "element": "#homeButton",
        "title": "interactiveTour.global.menu-button-title",
        "text": "interactiveTour.global.home-button",
        "onHide": {
          "action": "click"
        }
      },
      {
        "element": "#menu-button",
        "title": "interactiveTour.global.menu-button-title",
        "text": "interactiveTour.global.menu-button-close",
        "onHide": {
          "action": "click"
        }
      },
      {
        "element": "igo-geolocate-button",
        "title": "interactiveTour.global.geolocate-title",
        "text": "interactiveTour.global.geolocate"
      },
      {
        "element": "igo-baselayers-switcher",
        "title": "interactiveTour.global.baselayers-title",
        "text": "interactiveTour.global.baselayers"
      },
      {
        "title": "interactiveTour.global.end-title",
        "text": "interactiveTour.global.end"
      }
    ]
  },

  "timeFilter": {
    "title": "interactiveTour.timeFilter.title",
    "steps": [
      {
        "element": "div.igo-panel-title",
        "text": "interactiveTour.timeFilter.start"
      },
      {
        "element": "div.igo-panel-title",
        "text": "interactiveTour.timeFilter.end"
      }
    ]
  },

  "measurer": {
    "title": "interactiveTour.measurer.title",
    "steps": [
      {
        "element": "div.igo-panel-title",
        "text": "interactiveTour.measurer.start"
      },
      {
        "element": "div.measure-type-toggle",
        "text": "interactiveTour.measurer.type"
      },
      {
        "element": "div.measure-options",
        "text": "interactiveTour.measurer.options"
      },
      {
        "element": "div.measure-options mat-slide-toggle:first-child",
        "text": "interactiveTour.measurer.toggle-2"
      },
      {
        "element": "div.measure-options mat-slide-toggle:nth-child(2)",
        "text": "interactiveTour.measurer.toggle-3"
      },
      {
        "element": "div.measure-options mat-slide-toggle:last-child",
        "text": "interactiveTour.measurer.toggle-4"
      },
      {
        "element": "app-map-overlay",
        "text": "interactiveTour.measurer.map",
        "position": ""
      },
      {
        "element": "igo-measurer-item:first-of-type mat-form-field.measure-field",
        "text": "interactiveTour.measurer.measure-field"
      },
      {
        "element": "igo-measurer-item:first-of-type mat-form-field.unit-field",
        "text": "interactiveTour.measurer.measure-unit"
      },
      {
        "element": "mat-button-toggle:last-of-type button",
        "text": "interactiveTour.measurer.area-click",
        "onHide": {
          "action": "click"
        }
      },
      {
        "element": "igo-measurer-item:first-of-type mat-form-field.measure-field",
        "text": "interactiveTour.measurer.perimeter"
      },
      {
        "element": "igo-measurer-item:last-of-type mat-form-field.measure-field",
        "text": "interactiveTour.measurer.area-field"
      },
      {
        "element": "igo-measurer-item:last-of-type mat-form-field.unit-field",
        "text": "interactiveTour.measurer.area-unit"
      },
      {
        "element": "app-map-overlay",
        "text": "interactiveTour.measurer.map-2",
        "position": "",
        "onHide": {
          "action": "click",
          "waitFor": "tbody",
          "condition": "!tbody"
        }
      },
      {
        "element": "igo-entity-table.table-compact",
        "text": "interactiveTour.measurer.results"
      },
      {
        "element": "div.measure-store-buttons",
        "text": "interactiveTour.measurer.results-buttons"
      },
      {
        "element": "div.mat-checkbox-inner-container",
        "text": "interactiveTour.measurer.check-measure",
        "onShow": {
          "action": "click",
          "condition": "div.measure-store-buttons .mat-focus-indicator:disabled"
        }
      },
      {
        "element": "div.measure-store-buttons",
        "text": "interactiveTour.measurer.active-results-buttons"
      }
    ]
  },

  "spatialFilter": {
    "title": "interactiveTour.spatial-filter.title",
    "position": "auto",
    "steps": [
      {
        "element": "div.igo-panel-title",
        "text": "interactiveTour.spatial-filter.start",
        "onShow": {
          "element": "#mat-tab-label-0-0",
          "action": "click"
        }
      },
      {
        "element": "div.mat-tab-label-container",
        "text": "interactiveTour.spatial-filter.zone-type"
      },
      {
        "element": "div.mat-tab-labels div:first-of-type",
        "text": "interactiveTour.spatial-filter.predefined"
      },
      {
        "element": "div.mat-tab-labels div:nth-of-type(2)",
        "text": "interactiveTour.spatial-filter.drawing"
      },
      {
        "element": "div.mat-tab-body-content .mat-form-field",
        "text": "interactiveTour.spatial-filter.search-by"
      },
      {
        "element": "igo-spatial-filter-list .mat-form-field",
        "text": "interactiveTour.spatial-filter.zone"
      },
      {
        "element": "igo-spatial-filter-item .mat-radio-group",
        "text": "interactiveTour.spatial-filter.results-type"
      },
      {
        "element": "mat-radio-group mat-radio-button:first-of-type",
        "text": "interactiveTour.spatial-filter.address"
      },
      {
        "element": "mat-radio-group mat-radio-button:first-of-type",
        "text": "interactiveTour.spatial-filter.warning-address"
      },
      {
        "element": "mat-radio-group mat-radio-button:last-of-type",
        "text": "interactiveTour.spatial-filter.thematics"
      },
      {
        "element": "div.buttons .clear-search-button",
        "text": "interactiveTour.spatial-filter.initialize",
        "beforeShow": {
          "element": "div.mat-tab-labels div:first-of-type",
          "action":"click"
        }
      },
      {
        "element": "div.mat-tab-labels div:nth-of-type(2)",
        "text": "interactiveTour.spatial-filter.drawing-select",
        "onShow": {
          "action": "click"
        }
      },
      {
        "element": "div.spatial-type-toggle",
        "text": "interactiveTour.spatial-filter.drawing-type"
      },
      {
        "element": "div.header",
        "text": "interactiveTour.spatial-filter.activate-drawing"
      },
      {
        "element": "div.header",
        "text": "interactiveTour.spatial-filter.freehand-drawing"
      },
      {
        "element": "mat-sidenav-content",
        "text": "interactiveTour.spatial-filter.drawing-map",
        "position": ""
      },
      {
        "element": "button.clear-form-button",
        "text": "interactiveTour.spatial-filter.clear-drawing"
      },
      {
        "element": "button.search-button",
        "text": "interactiveTour.spatial-filter.launch"
      },
      {
        "element": "div.results",
        "text": "interactiveTour.spatial-filter.results-table"
      },
      {
        "element": "button.remove-button",
        "text": "interactiveTour.spatial-filter.clear-results"
      },
      {
        "element": "button.export-button",
        "text": "interactiveTour.spatial-filter.export"
      }
    ]
  }
}
