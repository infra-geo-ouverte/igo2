          "pushButtons": {
            "groups": [
              {"title": "Group 1 Title","name": "1","ids": ["id1","id2"]}
            ],
            "bundles": [
              {
                "id": "id1",
                "logical": "Or",
                "selectors": [
                  {
                    "title": "Bell",
                    "enabled": true,
                    "filters": {
                      "operator": "PropertyIsEqualTo",
                      "propertyName": "orgresp",
                      "expression": "Bell"
                    }
                  },
                  {
                    "title": "MTQ",
                    "enabled": false,
                    "filters": {
                      "operator": "PropertyIsEqualTo",
                      "propertyName": "orgresp",
                      "expression": "MTQ"
                    }
                  }
                ]
              }
            ]
        }}
        }
      }]
